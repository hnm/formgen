{
	"name" : "hnm/formgen",
	"type" : "n2n-tmpl-module",
	"description" : "Formular Generator module for n2n",
	"keywords" : [
		"n2n-tmpl-module",
		"n2n-module",
		"n2n"
	],
	"homepage" : "https://n2n.rocks/",
	"license" : "MIT",
	"authors" : [{
			"name" : "Thomas GÃ¼nther",
			"email" : "guenther@hnm.ch"
		}
	],
	"minimum-stability" : "dev",
	"prefer-stable": true,
	"config" : {
		"vendor-dir" : "src/vendor",
		"allow-plugins": {
			"n2n/n2n-composer-module-installer": true
		}
	},
	"require" : {
		"n2n/n2n-composer-module-installer" : "^7.1",
		"n2n/n2nutil-bootstrap" : "^4.3.3",
		"n2n/rocket" : "^3"
	},
	"autoload" : {
		"psr-4" : {
			"formgen\\" : "src/app/formgen"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"formgen\\" : "src/test/formgen"
		}
	},
	"require-dev" : {
		"n2n/hangar" : "^8.0.0",
		"phpunit/phpunit" : "^9.5"
	}
}
